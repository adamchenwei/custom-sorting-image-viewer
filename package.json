{
  "name": "custom-sorting-image-viewer",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "start": "next start -p 3087",
    "lint": "next lint",
    "process-images": "ts-node scripts/run-image-processing.ts",
    "dev1": "npm run process-images && next dev -p 3099",
    "dev": "npm run process-images && next dev",
    "prompt": "./scripts/consolidate-app-custom-sorting-image-viewer.sh ./app/ --exclude node_modules dist",
    "build": "npm run process-images && next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "image:clean-up-optimization-cache": "ts-node scripts/clean-optimization-cache.ts",
    "test:server": "npm run process-images && next dev --port 3087",
    "test:e2e": "playwright test",
    "test:ci": "npm run process-images && npm run build && source ~/.nvm/nvm.sh && nvm use && playwright test --reporter=list",
    "test:e2e:report": "npm run test:e2e && lsof -t -i:9323 | xargs kill -9 || true && npx playwright show-report",
    "get-total-files-count": "ts-node scripts/get-total-files-count.ts",
    "delete-folder": "ts-node scripts/delete-folder.ts",
    "upload-optimized-images-to-cloud-and-update-datajson": "ts-node scripts/upload-optimized-images-to-cloud.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/cloudfront-signer": "^3.957.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.469.0",
    "next": "14.2.13",
    "openai": "^4.29.1",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm": "^11.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}